// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model DIS {
  did             String @id
  content         String
  hexDid          String @unique
  salt            String
  signatureWallet String
  signatureDid    String
  didDocument     Json
}
